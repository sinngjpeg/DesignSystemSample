<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ButtonDesign">
        <attr name="buttonTypes" format="enum">
            <enum name="pink" value="0"/>
            <enum name="purple" value="1"/>
            <enum name="blue" value="2"/>
            <enum name="red" value="3"/>
        </attr>
        <attr name="isOutlined" format="boolean"/>
        <attr name="icon" format="reference"/>
        <attr name="label" format="string"/>
        <attr name="isEnabled" format="string"/>
    </declare-styleable>

</resources>